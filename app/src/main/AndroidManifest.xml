<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wel.wat.edu.pl.mojpiniadz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_apk_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_apk_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddExpensesActivity"></activity>
        <activity
            android:name=".PersonalDataActivity"
            android:parentActivityName=".SettingsActivity" />
        <activity
            android:name=".Set_PINActivity"
            android:parentActivityName=".SettingsActivity" />
        <activity
            android:name=".CategoryActivity"
            android:parentActivityName=".SettingsActivity" />
        <activity
            android:name=".CurrencyActivity"
            android:parentActivityName=".SettingsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AboutActivity"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>